<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.analizate.main"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="9"
        android:targetSdkVersion="17" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/analizatelogo"
        android:label="@string/app_name" >
        <activity
            android:name="com.analizate.main.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        
        <activity
            android:name="com.analizate.main.DashboardActivity" 
            android:configChanges="orientation" 
    		android:screenOrientation="portrait" 
            android:label="@string/app_name" >
        </activity>
        
        <activity
            android:name="com.analizate.main.DoctorActivity" 
            android:configChanges="orientation" 
    		android:screenOrientation="portrait" 
            android:label="@string/app_name" >
        </activity>
        
        <activity
            android:name="com.analizate.main.HospitalActivity" 
            android:configChanges="orientation" 
    		android:screenOrientation="portrait" 
            android:label="@string/app_name" >
        </activity>
        
        <activity
            android:name="com.analizate.main.PharmActivity" 
            android:configChanges="orientation" 
    		android:screenOrientation="portrait" 
            android:label="@string/app_name" >
        </activity>
        
        <activity
            android:name="com.analizate.main.InfoActivity" 
            android:configChanges="orientation" 
    		android:screenOrientation="portrait" 
            android:label="@string/app_name" >
        </activity>
        
        <activity
            android:name="com.analizate.main.SolicitudActivity" 
            android:configChanges="orientation" 
    		android:screenOrientation="portrait" 
            android:label="@string/app_name" >
        </activity>
    </application>

</manifest>